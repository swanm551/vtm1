    //<![CDATA[
    function updateTime() {
        const now = new Date();
        const timeString = now.toLocaleTimeString('en-US', { 
            hour: '2-digit', 
            minute: '2-digit',
            hour12: true 
        });
        const dateString = now.toLocaleDateString('en-GB', {
            day: '2-digit',
            month: '2-digit',
            year: '2-digit'
        });
        document.getElementById('currentTime').textContent = `${timeString}, ${dateString}`;
    }

    updateTime();
    setInterval(updateTime, 1000);

    let lastData = '';
    
    function fetchRates() {
        const url = 'https://docs.google.com/spreadsheets/d/e/2PACX-1vSNDdKNRmuS_lu66UUjPilT7lUNXogFk3ByljcyJHDRIUoPh5Lk_PCQ0dp7I5Td-YL55KWe1_WCeku5/pub?output=csv&gid=0';
        
        fetch(url)
            .then(function(response) {
                if (!response.ok) throw new Error('Network response was not ok');
                return response.text();
            })
            .then(function(data) {
                if (!data) throw new Error('No data received');
                
                if (data !== lastData) {
                    lastData = data;
                    
                    const rows = data.split('\n').map(function(row) {
                        return row.split(',').map(function(cell) {
                            return cell.trim();
                        });
                    });

                    const container = document.getElementById('ratesContainer');
                    
                    let tableHTML = '<table class="sheets-table">';
                    
                    if (rows.length > 0) {
                        tableHTML += '<thead><tr>';
                        rows[0].forEach(function(header, index) {
                            let colClass = '';
                            if (index === 0) colClass = 'col-currency';
                            else if (index === 1) colClass = 'col-rate';
                            else if (index === 2) colClass = 'col-time';
                            tableHTML += '<th class="' + colClass + '">' + header + '</th>';
                        });
                        tableHTML += '</tr></thead>';
                    }
                    
                    tableHTML += '<tbody>';
                    for (let i = 1; i < rows.length; i++) {
                        if (rows[i].length > 0) {
                            tableHTML += '<tr>';
                            rows[i].forEach(function(cell, index) {
                                let colClass = '';
                                let cellContent = cell;
                                if (index === 0) {
                                    colClass = 'col-currency';
                                }
                                else if (index === 1) {
                                    colClass = 'col-rate';
                                    cellContent = cell + ' Ks';
                                }
                                else if (index === 2) colClass = 'col-time';
                                tableHTML += '<td class="' + colClass + '">' + cellContent + '</td>';
                            });
                            tableHTML += '</tr>';
                        }
                    }
                    tableHTML += '</tbody></table>';
                    
                    container.innerHTML = tableHTML;

                    const table = container.querySelector('.sheets-table');
                    table.classList.add('rate-update');
                    setTimeout(() => {
                        table.classList.remove('rate-update');
                    }, 500);
                }
            })
            .catch(function(error) {
                console.error('Error:', error);
                document.getElementById('ratesContainer').innerHTML = 
                    '<div class="loading">Error loading data. Please refresh the page.</div>';
            });
    }

