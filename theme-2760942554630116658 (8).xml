        const container = document.getElementById('cardContainer');
        
        cards.forEach(card => {
            const cardElement = document.createElement('div');
            cardElement.className = 'card';
            cardElement.innerHTML = `
                <img src="${card.image}" alt="${card.title}">
                <div class="card-content">
                    <h3 class="card-title">${card.title}</h3>
                    <div class="card-details" id="${card.title.toLowerCase().replace(' ', '-')}">Loading...</div>
                </div>
            `;
            container.appendChild(cardElement);
            
            fetch(card.detailsUrl)
                .then(response => response.text())
                .then(data => {
                    document.getElementById(`${card.title.toLowerCase().replace(' ', '-')}`).innerHTML = data;
                })
                .catch(error => {
                    console.error('Error fetching card details:', error);
                    document.getElementById(`${card.title.toLowerCase().replace(' ', '-')}`).innerHTML = 
                        'Error loading details. Please refresh the page.';
                });
        });
    }

